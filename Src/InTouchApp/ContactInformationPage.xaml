        <CommandBar x:Name="MainCommandBar" ClosedDisplayMode="Compact">
            <CommandBar.PrimaryCommands>
                <!--CommandBarElementCollection-->
                    <AppBarButton x:Name="Edit" Icon="Edit" Label="Edit" />
                    <AppBarButton x:Name="Delete" Icon="Delete" Label="Delete" />
                    <AppBarButton x:Name="Share" Label="Share">
                    <AppBarButton.Icon>
                            <BitmapIcon UriSource="ms-appx:///Assets/Icons/in_ic_share_72px.png" />
                    </AppBarButton.Icon>
                    </AppBarButton>
                <!--/CommandBarElementCollection-->
            </CommandBar.PrimaryCommands>
        </CommandBar>
    </Page.BottomAppBar>
    
